{
  "keywords": [],
  "rules": [
    {
      "name": "DOB",
      "patterns": [
        "\\b(?:0?[1-9]|[12]\\d|3[01])[/-](?:0?[1-9]|1[0-2])[/-](?:19|20)\\d{2}\\b",
        "\\b(?:19|20)\\d{2}-(?:0?[1-9]|1[0-2])-(?:0?[1-9]|[12]\\d|3[01])\\b"
      ]
    },
    { "name": "SSN", "patterns": ["\\b\\d{3}-\\d{2}-\\d{4}\\b"] },
    { "name": "NPI", "patterns": ["\\b[12]\\d{9}\\b"] },
    { "name": "ICD10", "patterns": ["\\b[A-Z][0-9][A-Z0-9](?:\\.[A-Z0-9]{1,4})?\\b"] },
    { "name": "MRN", "patterns": ["\\bMRN[:\\s]*\\d{6,12}\\b"] },
    { "name": "IBAN", "patterns": ["\\b[A-Z]{2}\\d{2}[A-Z0-9]{10,30}\\b"] },
    { "name": "SWIFT-BIC", "patterns": ["\\b[A-Z]{6}[A-Z0-9]{2}(?:[A-Z0-9]{3})?\\b"] },
    { "name": "CreditCard", "patterns": ["\\b(?:\\d[ -]?){13,19}\\b"] },
    { "name": "IFSC", "patterns": ["\\b[A-Z]{4}0[A-Z0-9]{6}\\b"] },
    { "name": "PAN", "patterns": ["\\b[A-Z]{5}\\d{4}[A-Z]\\b"] },
    { "name": "RoutingUS", "patterns": ["\\b\\d{9}\\b"] },
    { "name": "Passport", "patterns": ["\\bpassport\\b.{0,20}\\b[A-Z0-9]{6,9}\\b"] },
    { "name": "DriversLicense", "patterns": ["\\bDL[-\\s]?[A-Z0-9]{5,15}\\b"] },
    { "name": "Aadhaar", "patterns": ["\\b\\d{4}\\s?\\d{4}\\s?\\d{4}\\b"] },
    { "name": "TIN", "patterns": ["\\bTIN[:\\s]?\\d{9}\\b"] },
    { "name": "GSTIN", "patterns": ["\\b[0-9A-Z]{15}\\b"] },
    { "name": "SensitiveLabels",
      "patterns": [        "\\b(?:Rama|Nagireddi|\\s+krish|rama)\\b"
      ]},
    { "name": "InternalIPs","patterns": [    "\\b(?:10\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}|172\\.(1[6-9]|2[0-9]|3[1])\\.[0-9]{1,3}\\.[0-9]{1,3}|192\\.168\\.[0-9]{1,3}\\.[0-9]{1,3})\\b"]},
   {  "name": "APICompanyLeaks","patterns": [        "\\b(?:api[_-]?key|secret|token|bearer|auth|password|pwd)[\\s:=]*['\\\"]?[a-zA-Z0-9]{16,64}['\\\"]?\\b",
       "\\b(?:https?://[^\\s]+\\.(?:company|internal|prod|staging|dev)\\.(?:com|io|net|org)|staging-api|prod-api)\\b",
       "\\b(?:x-api-key|Authorization:\\s*Bearer\\s+|api_key=)[a-zA-Z0-9_-]+\\b"
      ] }
  ]
}
